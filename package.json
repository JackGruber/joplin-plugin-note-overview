{
  "name": "joplin-plugin-note-overview",
  "version": "1.5.2",
  "scripts": {
    "dist": "webpack --joplin-plugin-config buildMain && webpack --joplin-plugin-config buildExtraScripts && webpack --joplin-plugin-config createArchive",
    "prepare": "npm run test && npm run dist && husky install",
    "update": "npm install -g generator-joplin && yo joplin --update",
    "compileTools": "tsc -p ./tools/tsconfig.json",
    "release": "npm run compileTools && node ./tools/createRelease.js",
    "publish": "npm publish && npm run gitRelease",
    "gitRelease": "node ./tools/createRelease.js --upload",
    "test": "jest"
  },
  "license": "MIT",
  "keywords": [
    "joplin-plugin"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^14.0.14",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "dotenv": "^10.0.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "mime": "^2.5.2",
    "on-build-webpack": "^0.1.0",
    "prettier": "2.3.0",
    "tar": "^6.0.5",
    "ts-jest": "^26.5.6",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.9",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "electron-log": "^4.4.1",
    "moment": "^2.29.1",
    "remark": "^13.0.0",
    "string-natural-compare": "^3.0.1",
    "strip-markdown": "^4.0.0",
    "yaml": "^1.10.2"
  },
  "browser": {
    "fs": false
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^api$": "<rootDir>/__test__/mocks/joplinApi.ts",
      "^api/(.*)$": "<rootDir>/api/$1"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
